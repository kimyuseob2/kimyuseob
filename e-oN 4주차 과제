def bubbleSort(arr):
    n = len(arr)
        
    for i in range(n):
        for j in range(0, n - i - 1):
            if arr[j] > arr[j + 1]: 
                arr[j], arr[j + 1] = arr[j + 1], arr[j]
                
                
arr = list(map(int,input("무작위 숫자들을 입력하시오.\n:").split(" ")))  

bubbleSort(arr)

print(arr)
